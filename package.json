{
  "name": "chitchat",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "prebuild": "eslint -c .eslintrc.json -p tsconfig.json --fix",
    "build": "tsc --project",
    "build-prod": "cd client && npm install && npm run build && cd ..",
    "server": "nodemon --verbose --watch \"server\" --ext \"ts,json\" --exec ts-node server/server.ts",
    "client": "cd client && npm start && cd ..",
    "dev": "concurrently \"npm run client\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derickfan/chitchat.git"
  },
  "author": "derickfan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/derickfan/chitchat/issues"
  },
  "homepage": "https://github.com/derickfan/chitchat#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "nodemon": "^2.0.13",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "socket.io": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
